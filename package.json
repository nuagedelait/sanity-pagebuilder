{
  "name": "@nuagedelait/sanity-pagebuilder",
  "version": "1.0.0",
  "description": "Sanity plugin to create a fast website admin with pagebuilder",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nuagedelait/sanity-pagebuilder"
  },
  "license": "MIT",
  "author": "nuagedelait <oliver@nuagedelait.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.4"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^3.3.6",
    "@sanity/plugin-kit": "^3.1.10",
    "@types/react": "^18.2.47",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "prettier-plugin-packagejson": "^2.4.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.5",
    "sanity": "^3.23.4",
    "styled-components": "^5.3.11",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=14"
  }
}
